{
  "name": "vscode-chat-plugin",
  "displayName": "VSCode Chat Plugin",
  "description": "Chat Plugin for VSCode",
  "version": "0.0.1",
  "publisher": "younesbami",
  "repository": "",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "*"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-chat-plugin.startChat",
        "title": "Start Chat"
      }
    ],
    "keybindings": [
      {
        "command": "vscode-chat-plugin.startChat",
        "key": "cmd+i",
        "mac": "cmd+i",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "package": "vsce package"
  },
  "dependencies": {
    "ws": "^8.2.3"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/node": "^14.0.0",
    "vsce": "^1.103.1"
  }
}